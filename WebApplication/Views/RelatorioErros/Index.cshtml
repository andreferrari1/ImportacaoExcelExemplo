@model IEnumerable<Domain.Entities.SheetAdmissionValidationErrors>

@{
    ViewData["Title"] = "Erros";
}

<h2>Favor corrigir os erros encontrados na planilha e processar novamente</h2>

<p>
    <a asp-action="Return">Voltar</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Line)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Errors)
            </th>

            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Line)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Errors)
                </td>
            </tr>
        }
    </tbody>
</table>
